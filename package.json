    "benchmark": "^2.1.1",
    "cross-env": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "tape": "^4.6.0",
    "updtr": "^0.2.1"
  },
  "engines": {
    "node": ">=4"
  }
